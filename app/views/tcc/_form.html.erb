<%= semantic_form_for @tcc do |f| %>
    <%= f.inputs do %>
        <%= f.input :name %>
        <%= f.input :leader %>
        <%= f.input :title %>
        <%= f.input :summary,  input_html: { rows: "4" } %>
        <%= f.input :presentation,  input_html: { rows: "4" }  %>
        <%= f.input :final_considerations,  input_html: { rows: "4" }  %>
    <% end %>

    <%= f.actions do %>
        <%= f.action :submit, :as => :input %>
    <% end %>
<% end %>
