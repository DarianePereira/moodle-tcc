<%= semantic_form_for @tcc do |f| %>
    <%= f.inputs do %>
        <%= f.input :moodle_user %>
        <%= f.input :title %>
        <%= f.input :summary %>
        <%= f.input :presentation %>
        <%= f.input :final_considerations %>
        <%= f.input :name %>
        <%= f.input :leader %>
    <% end %>

    <%= f.actions do %>
        <%= f.action :submit, :as => :input %>
    <% end %>
<% end %>
