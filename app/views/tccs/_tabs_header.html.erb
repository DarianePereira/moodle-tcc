<%= tabs_tag(:builder => ApplicationHelper::MenuTabBuilder) do |tab| %>
    <%= tab.data 'Dados',tccs_tabs_path(tab: 'data'), :class => 'active', id: 'data_tab' %>
    <%= tab.abstract 'Resumo', tccs_tabs_path(tab: 'abstract'), id: 'abstract_tab' %>
    <%= tab.presentation 'Apresentação', tccs_tabs_path(tab: 'presentation'), id: 'presentation_tab' %>
    <% TCC_CONFIG["hubs"].size.times do |i| %>
        <%= eval("tab.hub#{i+1} 'Eixo #{(i+1).to_s}', tccs_hubs_tabs_path(tab: 'hub', category: (i+1))") %>
    <% end %>
    <%= tab.final_considerations 'Considerações finais', tccs_tabs_path(tab: 'final_considerations'), id: 'final_consideration_tab' %>
    <%= tab.bibliographies 'Referências', tccs_tabs_path(tab: 'references'), id: 'bibliographies_tab' %>
<% end %>