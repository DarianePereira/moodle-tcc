<%= semantic_form_for @tcc do |f| %>
  <%= render :partial => 'tccs/forms/data', locals: { f: f } %>
  <%= render :partial => 'tccs/forms/abstract', locals: { f: f } %>
  <%= render :partial => 'tccs/forms/presentation', locals: { f: f } %>
  <%= render :partial => 'tccs/forms/hub1', locals: { f: f } %>
  <%= render :partial => 'tccs/forms/hub2', locals: { f: f } %>
  <%= render :partial => 'tccs/forms/hub3', locals: { f: f } %>
  <%= render :partial => 'tccs/forms/final_consideration', locals: { f: f } %>
  <%= render :partial => 'tccs/forms/bibliographies', locals: { f: f } %>

  <%= f.actions do %>
    <%= f.action :submit, :as => :input %>
  <% end %>
<% end %>