<%= semantic_form_for @course do |f| %>
  <%= f.inputs do %>
    <%= f.input :course_name %>
    <%= f.input :department_name %>
    <%= f.input :center_name %>
    <%= f.input :coordinator_name %>
    <%= f.input :presentation_data %>
    <%= f.input :approval_data %>
  <% end %>

  <%= f.actions do %>
    <%= f.action :submit, :as => :input %>
  <% end %>
<% end %>
